{
  "name": "blog",
  "version": "1.0.0",
  "description": "A programmer's web page using the mean stack",
  "keywords": [
    "OpenShift",
    "Node.js",
    "Jenkins",
    "Typescript",
    "Angular2",
    "Express"
  ],
  "author": {
    "name": "Abraham",
    "email": "contact@abrahammesa.com"
  },
  "watch": {
    "all": {
      "patterns": [
        "**/*.js"
      ],
      "ignore": "/built/**/*.js"
    }
  },
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "msms": "nodejs server.js",
    "watch": "npm-watch",
    "start": "npm run tsc:w | npm run msms",
    "all": "killall nodejs | npm start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brhmms/blog.git"
  },
  "engines": {
    "node": ">= 0.6.0",
    "npm": ">= 1.0.0"
  },
  "dependencies": {
    "express": "^4.13.3",
    "angular2": "2.0.0-beta.0",
    "systemjs": "0.19.6",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "0.5.10"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "jasmine-core": "2.4.1",
    "nodeunit": "^0.9.1",
    "npm-watch": "^0.1.1",
    "typescript": "^1.7.3"
  },
  "private": true,
  "main": "server.js"
}
